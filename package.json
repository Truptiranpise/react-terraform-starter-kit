{
  "name": "react-terraform-starter-kit",
  "version": "2.0.0",
  "description": "Restrict access to S3 files with an AWS API Gateway custom authorizer",
  "main": "index.js",
  "scripts": {
    "create": "npm run service && npm run init && npm run plan && npm run apply && npm run client && npm run sync && npm run open",
    "lint": "eslint *.js './service/src/**/*.js' './client/**/*.js' './client/**/*.jsx'",
    "sync": "s3p sync",
    "start": "webpack-dev-server --config ./client/webpack.dev.js",
    "client": "webpack --config ./client/webpack.prod.js",
    "service": "webpack -p --config ./service/webpack.config.js",
    "init": "cd ./service && terraform init",
    "plan": "cd ./service && terraform plan -out terraform.tfplan",
    "apply": "cd ./service && terraform apply terraform.tfplan && npm run dump",
    "destroy": "cd ./service && terraform destroy",
    "state": "terraform output -state=./service/terraform.tfstate",
    "dump": "npm run --silent state -- -json > ./client/data/terraform_output.json",
    "open": "node -e \"require('opn')(require('./client/data/terraform_output.json').api_url.value); process.exit();\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamjarret/react-terraform-starter-kit.git"
  },
  "author": "Adam Jarret <adam@atj.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adamjarret/react-terraform-starter-kit/issues"
  },
  "homepage": "https://github.com/adamjarret/react-terraform-starter-kit#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.19",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "^1.0.0",
    "eslint": "^5.7.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "mustache-loader": "^1.4.1",
    "opn": "^5.4.0",
    "s3-publish": "^1.0.9",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4",
    "write-file-webpack-plugin": "^4.4.1"
  },
  "dependencies": {
    "@material-ui/core": "^3.2.2",
    "@material-ui/icons": "^3.0.1",
    "ky": "^0.4.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  }
}
