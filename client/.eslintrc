{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true
  },
  "rules": {
    "no-console": 0,
    "no-debugger": 1
  },
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "babel"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}
